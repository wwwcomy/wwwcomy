<?xml version="1.0" encoding="UTF-8"?>
<Haiyan xsi:noNamespaceSchemaLocation="../../haiyan.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<table name="T_DIC_COMMON_PRODUCT_TYPE" description="通用字典-产品分类-类型分类" realTable="T_DIC_COMMON">
		<!--用于所有T_DIC_PRODUCT的"产品类型"字段；初始时包含软件产品和服务产品2个值-->
		<id name="ID" javaType="string"/>
		<field name="CODE" javaType="string" description="编号">
			<component type="text"/>
		</field>
		<field name="NAME" javaType="string" description="名称">
			<component type="text"/>
		</field>
		<field name="MEMO" javaType="string" description="备注">
			<component type="textArea" colspan="3"/>
		</field>
		<queryFilter>
			<pluggedFilter> and t_1.LABEL='CPFL' and t_1.LABEL2='LXFL'</pluggedFilter>
		</queryFilter>
	</table>
</Haiyan>
